<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="BorderButton" parent="Widget.AppCompat.Button">
        <item name="android:textColor">#9d9d9d</item>
        <item name="strokeColor">#9d9d9d</item>
        <item name="strokeWidth">10dp</item>
        <item name="backgroundTint">#606060</item>
    </style>

    <style name="OutlineSpinner" parent="Widget.AppCompat.Spinner">
        <item name="strokeColor">@color/lightGrey</item>
        <item name="strokeWidth">3dp</item>
        <item name="backgroundColor">@color/lightOrange</item>
    </style>

    <style name="roundedImageViewRounded">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">50%</item>
    </style>

</resources>